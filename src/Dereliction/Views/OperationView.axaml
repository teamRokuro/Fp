<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Dereliction.Views.OperationView">
    <Grid ColumnDefinitions="1*,1*,1*" RowDefinitions="30,30,1*" Margin="4">
        <Label Content="Input" Grid.Row="0" Grid.Column="0"/>
        <TreeView Grid.Row="2" Grid.Column="0">
            <TreeViewItem Header="V0">
                <TreeViewItem Header="V0-0"></TreeViewItem>
            </TreeViewItem>
            <TreeViewItem Header="V1"></TreeViewItem>
        </TreeView>
        <Label Content="Output" Grid.Row="0" Grid.Column="1"/>
        <TreeView Grid.Row="2" Grid.Column="1">
            <TreeViewItem Header="V0">
                <TreeViewItem Header="V0-0"></TreeViewItem>
            </TreeViewItem>
            <TreeViewItem Header="V1"></TreeViewItem>
        </TreeView>
        <Label Content="Log" Grid.Row="0" Grid.Column="2"/>
        <TextBox Grid.Row="2" Grid.Column="2" AcceptsReturn="True" TextWrapping="Wrap" />
    </Grid>
</UserControl>
